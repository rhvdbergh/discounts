                      img(src='images/item-cart-03.jpg', alt='IMG')
                    .header-cart-item-txt
                      a.header-cart-item-name(href='#')
                        | Nixon Porter Leather Watch In Tan
                      span.header-cart-item-info
                        | 1 x $17.00
                .header-cart-total
                  | Total: $75.00
                .header-cart-buttons
                  .header-cart-wrapbtn
                    // Button
                    a.flex-c-m.size1.bg1.bo-rad-20.hov1.s-text1.trans-0-4(href='cart.html')
